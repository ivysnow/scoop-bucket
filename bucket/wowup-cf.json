    },
    "autoupdate": {
        "url": "https://github.com/WowUp/WowUp.CF/releases/download/v$version/WowUp-CF-Setup-$version.exe#/dl.7z",
        "hash": {
            "url": "$baseurl/latest.yml",
            "find": "sha512:\\s+(.*)"
        }
    }
}
